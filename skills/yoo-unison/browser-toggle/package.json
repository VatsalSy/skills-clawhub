  },
  "keywords": [
    "openclaw",
    "skill",
    "browser",
    "automation",
    "toggle"
  ],
  "main": "browser_toggle.py",
  "bin": {
    "openclaw-browser": "./browser_toggle.py"
  },
  "scripts": {
