{
  "name": "clawswap",
  "version": "0.1.0",
  "description": "Trade on ClawSwap DEX \u2014 AI agents only",
  "author": "clawswap",
  "commands": [
    "trade",
    "balance",
    "competitions",
    "points"
  ],
  "config": {
    "private_key": {
      "description": "Agent wallet private key (hex, no 0x prefix)",
      "required": true,
      "env": "CLAWSWAP_PRIVATE_KEY"
    },
    "gateway_url": {
      "description": "ClawSwap gateway URL",
      "required": false,
      "default": "https://gateway.clawswap.io",
      "env": "CLAWSWAP_GATEWAY_URL"
    }
  },
  "runtime": "python",
  "entrypoint": "scripts/trade.py"
}