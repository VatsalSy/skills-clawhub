        "comma": "，",
        "semicolon": "；",
        "colon": "：",
        "quotation_mark": "\"\"\"\"",
        "question_mark": "？",
        "exclamation_mark": "！",
        "parentheses": "()",
        "brackets": "[]",
        "braces": "{}",
        "dash": "—",
        "enumeration_point": "、"
      }
    },
    "abbreviation_rules": {
      "en": {
        "volume": "vol.",
        "number": "no.",
        "page": "p.",
        "pages": "pp.",
        "chapter": "chap.",
        "edition": "ed.",
        "revised": "rev.",
        "translator": "trans.",
        "editor": "ed.",
        "editors": "eds.",
        "anonymous": "Anon."
      },
      "zh": {
        "volume": "卷",
        "number": "期",
        "page": "页",
        "pages": "页",
        "chapter": "章",
        "edition": "版",
        "revised": "修订版",
        "translator": "译",
        "editor": "编",
        "editors": "编",
        "anonymous": "佚名"
      }
    }
  },
  "special_cases": {
    "multiple_authors_same_year": {
      "apa": {
        "rule": "Add alphabetical suffix (a, b, c...) to year",
        "example": "Smith (2023a), Smith (2023b)"
      },
      "chicago": {
        "rule": "Same as APA",
        "example": "Smith (2023a), Smith (2023b)"
      },
      "harvard": {
        "rule": "Same as APA",
        "example": "Smith (2023a), Smith (2023b)"
      },
      "mla": {
        "rule": "Add short title suffix",
        "example": "Smith (Title A 23), Smith (Title B 23)"
      },
      "ieee": {
        "rule": "Use separate citation numbers",
        "example": "[1], [2], [3]"
      }
    },
    "no_author": {
      "apa": {
        "rule": "Use title in parentheses",
        "example": "(\"Article Title\", 2023)"
      },
      "chicago": {
        "rule": "Use title in parentheses",
        "example": "(\"Article Title\", 2023)"
      },
      "harvard": {
        "rule": "Use title in parentheses",
        "example": "(\"Article Title\", 2023)"
      },
      "mla": {
        "rule": "Use title in quotes",
        "example": "\"Article Title\""
      },
      "ieee": {
        "rule": "Use title with brackets",
        "example": "[1] \"Article Title\""
      }
    },
    "no_date": {
      "apa": {
        "rule": "Use n.d. for no date",
        "example": "Smith (n.d.)"
      },
      "chicago": {
        "rule": "Use n.d. for no date",
        "example": "Smith (n.d.)"
      },
      "harvard": {
        "rule": "Use n.d. for no date",
        "example": "Smith (n.d.)"
      },
      "mla": {
        "rule": "Use n.d. for no date",
        "example": "Smith (n.d.)"
      },
      "ieee": {
        "rule": "Use available date",
        "example": "[1] (accessed 2023-06-01)"
      }
    }
  },
