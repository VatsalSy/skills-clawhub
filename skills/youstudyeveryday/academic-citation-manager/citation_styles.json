  "formatting_rules": {
    "author_name_order": {
      "western": {
        "family_first": "{family}, {given}",
        "given_first": "{given} {family}",
        "initials": "{family}, {given_initial}."
      },
      "chinese": {
        "family_given": "{family}{given}",
        "given_family": "{given}{family}"
      }
    },
    "title_case": {
      "sentence_case": {
        "description": "Only first word and proper nouns capitalized",
        "rule": "capitalize first letter of each sentence, keep proper nouns as is"
      },
      "title_case": {
        "description": "First letter of each word capitalized",
        "rule": "capitalize first letter of each word, except articles and short prepositions"
      },
      "upper_case": {
        "description": "All letters capitalized",
        "rule": "convert all letters to uppercase"
      },
      "lower_case": {
        "description": "All letters lowercase",
        "rule": "convert all letters to lowercase"
      },
      "original": {
        "description": "Keep original case",
        "rule": "do not change case"
      }
    },
    "punctuation": {
      "en": {
        "period": ".",
        "comma": ",",
        "semicolon": ";",
        "colon": ":",
        "apostrophe": "'",
        "quotation_mark": "\"",
        "question_mark": "?",
        "exclamation_mark": "!",
        "parentheses": "()",
        "brackets": "[]",
        "braces": "{}",
        "dash": "-",
        "em_dash": "—"
      },
