{
  "last_updated": "2026-02-22",
  "source": "bundled — update via https://github.com/stellawuellner/math-worksheets-skill",
  "tiers": {
    "FOUND_REASONING": {
      "description": "Reasoning models — step-by-step internal chain-of-thought, highest math accuracy",
      "patterns": [
        { "alias": "o3",        "match": "openai/o3" },
        { "alias": "o1",        "match": "openai/o1" },
        { "alias": "deepthink", "match": "gemini-2.5-pro-deepthink" },
        { "alias": "deepthink", "match": "gemini-2.0-pro-deepthink" },
        { "alias": "deepseek",  "match": "deepseek-r1" }
      ]
    },
    "FOUND_STRONG": {
      "description": "Strong non-reasoning models — excellent math and LaTeX quality, use when no reasoning model available",
      "patterns": [
        { "alias": "opus", "match": "claude-opus-4" },
        { "alias": "opus", "match": "claude-opus-3-5" }
      ]
    }
  }
